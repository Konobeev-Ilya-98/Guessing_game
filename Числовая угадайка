import random
from math import *

hidden_number = random.randint(1, 100) 

#–≤—ã–∑—ã–≤–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é –ø–æ–¥—Å—á—ë—Ç–∞
def guessing_game(user_number): 
    
        global hidden_number
        different_number =  abs(int(user_number) - hidden_number) 
    
        if int(user_number) == hidden_number:
            print('–£—Ä–∞! –ú–æ–∏ –ø–æ–∑–¥—Ä–∞–≤–ª–µ–Ω–∏—è, –≤—ã —É–≥–∞–¥–∞–ª–∏!!!')
            return True 
        elif int(user_number) > hidden_number:
            if different_number <= 5: 
                print('–ü–æ—á—Ç–∏, –Ω–æ –º–æ—ë —á–∏—Å–ª–æ –Ω–µ–º–Ω–æ–≥–æ –º–µ–Ω—å—à–µ') 
            else:   
                print('–ú–Ω–æ–≥–æ–≤–∞—Ç–æ, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑')
        elif int(user_number) < hidden_number:
            if different_number <= 5: 
                print('–ü–æ—á—Ç–∏, –Ω–æ –º–æ—ë —á–∏—Å–ª–æ –Ω–µ–º–Ω–æ–≥–æ –±–æ–ª—å—à–µ') 
            else:   
                print('–ú–∞–ª–æ–≤–∞—Ç–æ, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑') 
        return False     

#—Å—á–∏—Ç—ã–≤–∞–µ–º –¥–∞–Ω–Ω—ã–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
print('–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å –≤ —á–∏—Å–ª–æ–≤—É—é —É–≥–∞–¥–∞–π–∫—É!', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ —É–≥–∞–¥–∞—Ç—å –∑–∞–≥–∞–¥–∞–Ω–Ω–æ–µ –º–Ω–æ–π —á–∏—Å–ª–æüôÉ', sep = '\n') 
print('–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ –æ—Ç 1 –¥–æ 100') 


#–ø—Ä–æ–≤–µ—Ä—è–µ–º –ø—Ä–∞–≤–∏–ª—å–Ω–æ—Å—Ç—å –≤–≤–µ–¥—ë–Ω–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö
while True:
        n = input() 
        if n.isdigit() == False:
            print('–Ø —Ç–∞–∫ –Ω–µ –∏–≥—Ä–∞—é - —ç—Ç–æ –Ω–µ —á–∏—Å–ª–æüôÅ', '–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑', sep ='\n') 
        elif n.isdigit() and (1 > int(n) or int(n) > 100):
            print('–Ø,  –∫–æ–Ω–µ—á–Ω–æ, –Ω–µ –º–∞—Ç–µ–º–∞—Ç–∏–∫, –Ω–æ –∫–∞–∂–µ—Ç—Å—è —ç—Ç–æ —á–∏—Å–ª–æ –Ω–µ –≤ –¥–∏–∞–ø–∞–∑–æ–Ω–µ –æ—Ç 1 –¥–æ 100ü§î –ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑') 
        else:   
            if guessing_game(n):
                break
